<hibernate-mapping>
    <class name="main.java.ru.meeting.db.entity.UserRoutes" table="UserRoutes">
        <id column="id" name="id" type="java.lang.Long">
            <generator class="increment"/>
        </id>
        <property column="idUser" name = "route" type="java.lang.Long"/>
        <property column="idRoute" name = "route" type="java.lang.Long"/>

        <key column="id"/>
    </class>
</hibernate-mapping>